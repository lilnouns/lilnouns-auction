    "elliptic": ">=6.6.0",
    "path-to-regexp": ">=6.3.0",
    "rollup": ">=4.22.4",
    "secp256k1": ">=5.0.1",
    "semver": ">=7.5.2",
    "tar": ">=6.2.1",
    "ws": ">=7.5.10"
  },
  "funding": [
    {
      "type": "custom",
      "url": "https://ud.me/nekofar.crypto"

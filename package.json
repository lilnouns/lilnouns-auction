    "prettier-plugin-prisma": "5.0.0",
    "prettier-plugin-tailwindcss": "0.6.11",
    "prettier-plugin-toml": "2.0.1",
    "rimraf": "6.0.1",
    "turbo": "2.4.2"
  },
  "resolutions": {
    "@babel/traverse": ">=7.23.2",
    "cross-spawn": ">=7.0.5",
    "debug": ">=4.3.1",
    "elliptic": ">=6.6.0",

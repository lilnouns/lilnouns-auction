{
  "name": "lilnouns-auction",
  "main": ".open-next/worker.js",
  "compatibility_date": "2025-02-28",
  "compatibility_flags": ["nodejs_compat"],
  "placement": {
    "mode": "smart"
  },
  "env": {
    "preview": {
      "vars": {
        "NEXT_PUBLIC_BLOCK_EXPLORER": "https://sepolia.etherscan.io",
        "NEXT_PUBLIC_CHAIN_ID": "11155111",
        "NEXT_PUBLIC_SITE_URL": "https://sepolia.lilnouns.auction",
        "NODE_VERSION": "22.3.0"
      }
    },
    "production": {
      "vars": {
        "NEXT_PUBLIC_BLOCK_EXPLORER": "https://etherscan.io",
        "NEXT_PUBLIC_CHAIN_ID": "1",
        "NEXT_PUBLIC_SITE_URL": "https://lilnouns.auction",
        "NODE_VERSION": "22.3.0"
      }
    }
  },
  "assets": {
    "directory": ".open-next/assets",
    "binding": "ASSETS"
  }
}

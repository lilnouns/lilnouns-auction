{
  "name": "@repo/farcaster",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm codegen",
    "build": "wrangler deploy --dry-run --outdir dist",
    "codegen": "graphql-codegen --config codegen.config.ts",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --test-scheduled",
    "generate-types": "wrangler types --include-runtime=false --strict-vars=false --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "start": "wrangler dev --test-scheduled",
    "type-check": "pnpm generate-types && tsc"
  },
  "dependencies": {
    "@nekofar/lilnouns": "1.0.0-alpha.49",
    "@nekofar/warpcast": "2.0.0-alpha.109",
    "graphql": "16.12.0",
    "graphql-request": "7.4.0",
    "graphql-tag": "2.12.6",
    "luxon": "3.7.2",
    "remeda": "2.32.0",
    "viem": "2.43.3"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "1.15.2",
    "@cloudflare/workers-types": "4.20251229.0",
    "@graphprotocol/graph-ts": "0.38.2",
    "@graphql-codegen/cli": "6.1.0",
    "@graphql-codegen/schema-ast": "5.0.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-graphql-request": "6.3.0",
    "@graphql-codegen/typescript-operations": "5.0.7",
    "@parcel/watcher": "2.5.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "24.10.4",
    "dotenv": "17.2.3",
    "graphql-scalars": "1.25.0",
    "typescript": "5.9.3",
    "wrangler": "4.16.1"
  }
}

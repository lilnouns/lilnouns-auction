{
  "name": "@repo/farcaster",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm codegen",
    "build": "wrangler deploy --dry-run --outdir dist",
    "codegen": "graphql-codegen --config codegen.config.ts",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev --test-scheduled",
    "generate-types": "wrangler types --include-runtime=false --strict-vars=false",
    "start": "wrangler dev --test-scheduled",
    "type-check": "pnpm generate-types && tsc"
  },
  "dependencies": {
    "@nekofar/lilnouns": "1.0.0-alpha.6",
    "@nekofar/warpcast": "2.0.0-alpha.23",
    "graphql": "16.11.0",
    "graphql-request": "6.1.0",
    "graphql-tag": "2.12.6",
    "luxon": "3.6.1",
    "remeda": "2.22.3",
    "viem": "2.30.5"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "1.12.16",
    "@cloudflare/workers-types": "4.20250521.0",
    "@graphprotocol/graph-ts": "0.38.1",
    "@graphql-codegen/cli": "5.0.6",
    "@graphql-codegen/schema-ast": "4.1.0",
    "@graphql-codegen/typescript-graphql-request": "6.2.0",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@parcel/watcher": "2.5.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "22.15.26",
    "graphql-scalars": "1.24.2",
    "typescript": "5.8.3",
    "wrangler": "4.16.1"
  }
}
